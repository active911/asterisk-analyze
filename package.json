{
  "name": "asterisk-analyze",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "jshintConfig": {
    "esversion": 6
  },
  "scripts": {
    "//": "DEV RULES",
    "test": "mocha",
    "lint": "jshint --reporter=node_modules/jshint-stylish server.js etl.js src/app.js",
    "webpack": "NODE_ENV=production webpack",
    "build": "npm run lint -s && npm run test -s && npm run webpack -s",
    "watch:webpack": "webpack --progress --colors --watch",
    "browsersync": "browser-sync start --proxy 'localhost:3000' --files 'public/*' 'server.js' --port 3001",
    "watch:server": "nodemon -w server.js server.js | ./node_modules/bunyan/bin/bunyan",
    "watch:lint": "nodemon -w server.js -w etl.js -w src/app.js -x npm run lint -s",
    "dev": "parallelshell 'npm run watch:lint -s' 'npm run watch:server -s' 'npm run browsersync -s' 'npm run watch:webpack -s'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/active911/asterisk-analyze.git"
  },
  "author": "c0d3",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/active911/asterisk-analyze/issues"
  },
  "homepage": "https://github.com/active911/asterisk-analyze#readme",
  "dependencies": {
    "always-tail2": "^1.0.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "bunyan": "^1.8.5",
    "crossfilter": "^1.3.12",
    "d3": "^3.0.0",
    "dc": "^2.1.1",
    "express": "^4.14.0",
    "ioredis": "^2.4.3",
    "jquery": "^3.1.1",
    "line-by-line": "^0.1.5",
    "moment": "^2.17.1",
    "morgan": "^1.7.0",
    "nconf": "^0.8.4",
    "promise-mysql": "^3.0.0",
    "syslogd": "^1.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "bootstrap": "^3.3.7",
    "browser-sync": "^2.18.6",
    "bunyan-pretty": "0.0.1",
    "css-loader": "^0.26.1",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.8.3",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.2.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "parallelshell": "^2.0.0",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.5",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0-rc.4"
  }
}
